<!doctype html>
<html>
<head>
    <title>Add project</title>
</head>
<body>
<div class="container">
<div class="col-sm-6 col-sm-offset-3">


    <form action="/profile/addproject" method="post">
        <div class="form-group">
            <label>Naziv</label>
            <input type="text" class="form-control" name="naziv_projekta">
        </div>
        <div class="form-group">
            <label>Opis</label>
            <input type="text" class="form-control" name="opis_projekta">
        </div>
        <div class="form-group">
            <label>Cijena</label>
            <input type="number" min="0" class="form-control" name="cijena_projekta">
        </div>
        <div class="form-group">
            <label>Obavljeno</label>
            <input type="text" class="form-control" name="obavljeni_poslovi">
        </div>
        <div class="form-group">
            <label>Pocetak</label>
            <input type="date" class="form-control" name="datum_pocetak">
        </div>
        <div class="form-group">
            <label>Zavrsetak</label>
            <input type="date" class="form-control" name="datum_zavrsetak">
        </div>
        <div class="form-group">
            <label>Clanovi:</label>
                 <% for(var i=0; i < users.length; i++) { %>
                    <input type="checkbox" name="clanovi[]"  value=<%= users[i].local.email %>><%= users[i].local.email %>
                <% } %>
        </div>
        <button type="submit" class="btn btn-warning btn-lg">Submit</button>
    </form>

</div>
</div>
</body>
</html>